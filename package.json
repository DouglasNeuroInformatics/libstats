{
  "name": "@douglasneuroinformatics/libstats",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.7.0",
  "license": "Apache-2.0",
  "exports": {
    ".": {
      "types": "./lib/index.d.cts",
      "default": "./lib/index.cjs"
    }
  },
  "engines": {
    "node": ">= 20"
  },
  "scripts": {
    "build": "cargo-cp-artifact --npm cdylib ./dist/index.node -- cargo build --message-format=json-render-diagnostics"
  },
  "devDependencies": {
    "@douglasneuroinformatics/eslint-config": "^5.1.0",
    "@douglasneuroinformatics/prettier-config": "^0.0.1",
    "@douglasneuroinformatics/tsconfig": "^1.0.2",
    "cargo-cp-artifact": "^0.1.9",
    "eslint": "^9.9.1",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9.x"
      }
    },
    "supportedArchitectures": {
      "os": [
        "darwin",
        "linux"
      ],
      "cpu": [
        "arm64",
        "x64"
      ],
      "libc": [
        "musl"
      ]
    }
  },
  "prettier": "@douglasneuroinformatics/prettier-config"
}
